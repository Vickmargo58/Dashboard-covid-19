{
  "version": 2,
  "builds": [
    {
      "src": "src/main.py",
      "use": "@vercel/python",
      "config": {
        "runtime": "python3.9.18"
      }
    },
    {
      "src": "src/static/**",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/src/main.py"
    },
    {
      "src": "/(.*)",
      "dest": "/src/static/index.html"
    }
  ]
}
